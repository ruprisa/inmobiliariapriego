{% extends '::base.html.twig' %}

{% block body -%}
    <div class="hero-unit">
        <h3>{% trans from "inmuebles" %}inmu.new{% endtrans %}</h3>
    </div>

    {{ form_start(form) }}
    {{ form_errors(form) }}

    <div class="row-fluid">
        <div class="span4">
            {{ form_row(form.ref) }}
        </div>
        <div class="span4">
            {{ form_row(form.tipo) }}
        </div>
        <div class="span4">
            {{ form_row(form.descripcion) }}
        </div>
    </div>
    <div class="row-fluid">
        <div class="span4">
            {{ form_row(form.zona) }}
        </div>
        <div class="span4">
            {{ form_row(form.planta) }}
        </div>
        <div class="span4">
            {{ form_row(form.calle) }}
        </div>
    </div>
    <div class="row-fluid">
        <div class="span4">
            {{ form_row(form.poblacion) }}
        </div>
        <div class="span4">
            {{ form_row(form.provincia) }}
        </div>
        <div class="span4">
            {{ form_row(form.pais) }}
        </div>
    </div>
    <div class="row-fluid">
        <div class="span4">
            {{ form_row(form.terraza) }}
        </div>
        <div class="span4">
            {{ form_row(form.metrosterraza) }}
        </div>
        <div class="span4">
            {{ form_row(form.ascensor) }}
        </div>
    </div>
    <div class="row-fluid">
        <div class="span4">
            {{ form_row(form.jardin) }}
        </div>
        <div class="span4">
            {{ form_row(form.metrosjardin) }}
        </div>
        <div class="span4">
            {{ form_row(form.garaje) }}
        </div>
    </div>
    <div class="row-fluid">
        <div class="span4">
            {{ form_row(form.trastero) }}
        </div>
        <div class="span4">
            {{ form_row(form.metrostrastero) }}
        </div>
        <div class="span4">
            {{ form_row(form.obranueva) }}
        </div>
    </div>
    <div class="row-fluid">
        <div class="span4">
            {{ form_row(form.habitacionesdobles) }}
        </div>
        <div class="span4">
            {{ form_row(form.habitacionessimples) }}
        </div>
        <div class="span4">
            {{ form_row(form.banyos) }}
        </div>
    </div>

    <div class="row-fluid">
        <div class="span4">
            {{ form_row(form.superficie) }}
        </div>
        <div class="span4">
            {{ form_row(form.operacion) }}
        </div>
        <div class="span4">
            {{ form_row(form.precio) }}
        </div>
    </div>
    <div class="row-fluid">
        <div class="span4">
            {{ form_row(form.img) }}
        </div>
        <div class="span4">
            {{ form_row(form.destacado) }}
        </div>
        <div class="span4">

        </div>
    </div>

    <div class="row-fluid">
        <div class="well span12 row">
            <div class="span8">
                <a href="{{ path('inmueble') }}" class="btn btn-info btn-small" role="button">{% trans %}back.list{% endtrans %}</a>
            </div>
            <div class="span2">
                
            </div>
            <div class="span2">
                {{ form_row(form.submit) }}
            </div>
        </div>
    </div>
{{ form_rest(form) }}
{% endblock %}
